/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
	Not(in=sel,out=notSel);
	And(a=a,b=notSel,out=c);
	And(a=b,b=sel,out=d);
	Or(a=c,b=d,out=out);
}